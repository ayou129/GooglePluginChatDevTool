{
  "manifest_version": 3,
  "name": "ChatDevTool",
  "version": "1.0",
  "icons": {
    "128": "Google128x128.png"
  },
  "description": "a tool to help efficiently use the chat llm model",
  "permissions": [
    "tabs",
    "activetab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}